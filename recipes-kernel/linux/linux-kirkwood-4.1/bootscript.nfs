setenv loadaddr @UBOOT_LOADADDRESS@
setenv fdtaddr @UBOOT_FDTADDRESS@
setenv bootargs_nfs setenv bootargs ${bootargs_base} ip=dhcp root=/dev/nfs ro
setenv bootcmd_nfs 'run bootargs_nfs; tftp ${loadaddr} @KERNEL_IMAGETYPE@s-3.14/${uimage}; tftp ${fdtaddr} @KERNEL_IMAGETYPE@s-3.14/${fdt_file}; bootz ${loadaddr} - ${fdtaddr}'
run bootcmd_nfs
